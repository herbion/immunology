#{extends 'CRUD/layout.html' /}
#{set title:messages.get('objects.clinicalmanifestationcomplaints') /}

<div id="crudList" class="${type.name}">

    <h2 id="crudListTitle">&{'objects.clinicalmanifestationcomplaints'}</h2>

    <div id="crudListSearch">
        #{crud.search /}
    </div>

    <div id="crudListTable">
        #{crud.table fields:['clinicalManifestationComplainName']/}
    </div>

    <div id="crudListPagination">
        &{'total', models.ClinicalManifestationComplaint.count()}
        #{pagination /}
    </div>

    <p id="crudListAdd">
        <a href="@{blank()}">&{'clinicalmanifestationcomplaint.add'}</a>
    </p>

</div>
